"UnlitGeneric"
{
	"$additive" 1
	"$translucent" 1
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$decal" 1
	"$basetexturetransform"	"center .5 .5 scale 3 5 rotate 0  translate 0 0"
	"$alpha" 1	

	"$basetexture" "vgui\splinter_cell\noise"


        "Proxies"
        {
                "AnimatedTexture"
                {
                         "animatedtexturevar" "$basetexture"
                         "animatedtextureframenumvar" "$frame"
                         "animatedtextureframerate" 45.00
                }

        }
}
